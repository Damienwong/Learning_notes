{
  "application": {
    "name": "My Python App",
    "version": "1.3.0",
    "debug": false,
    "environment": "production",
    "max_threads": 8,
    "timeout": 30.5
  },
  "database": {
    "host": "db.example.com",
    "port": 5432,
    "username": "admin",
    "password": "s3cr3tP@ss",
    "database_name": "app_db",
    "use_ssl": true,
    "connection_pool": {
      "min_connections": 2,
      "max_connections": 10,
      "timeout": 5
    }
  },
  "logging": {
    "level": "INFO",
    "file_path": "/var/log/myapp/app.log",
    "max_size": "10MB",
    "backup_count": 5
  },
  "features": {
    "enabled_features": ["authentication", "notifications", "analytics"],
    "experimental": {
      "feature_x": false,
      "feature_y": true
    }
  },
  "api_endpoints": {
    "users": "https://api.example.com/v1/users",
    "products": "https://api.example.com/v1/products",
    "orders": "https://api.example.com/v1/orders"
  },
  "user_preferences": {
    "default_theme": "dark",
    "language": "en-US",
    "notifications": {
      "email": true,
      "push": true,
      "sms": false
    },
    "refresh_interval": 300
  },
  "whitelisted_ips": [
    "192.168.1.1",
    "192.168.1.5",
    "10.0.0.2"
  ],
  "environment_variables": {
    "CACHE_DIR": "${HOME}/.cache/myapp",
    "TEMP_DIR": "/tmp/${APP_NAME}"
  }
}